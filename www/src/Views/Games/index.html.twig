{% extends "Layouts/main.html.twig" %}

{% block content %}
<h2>List of all fetched Games</h2>
<ul class="list-disc">
  {% for game in games %}
    <li>
      <strong>
          <a class="text-neutral-300 text-lg" href="/games/show.php?{{ { 'gameid': game.id}|url_encode }}">
            {{ game.name }} ({{ game.total_achievements }})
          </a>
      </strong>
    </li>
  {% endfor %}
</ul>
{% endblock %}
